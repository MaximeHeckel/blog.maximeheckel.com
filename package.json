{
  "name": "blog.maximeheckel.com",
  "private": true,
  "description": "blog.maximeheckel.com - Blog",
  "version": "0.5.0",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build:dev": "next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "dev": "next-remote-watch ./content",
    "export": "next export",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "generate:index": "node ./scripts/generate-index",
    "generate:og": "node ./scripts/generate-opengraph-images",
    "generate:rss": "node ./scripts/generate-rss",
    "generate:sitemap": "node ./scripts/generate-sitemap",
    "initialize": "echo \"@maximeheckel:registry=https://npm.pkg.github.com\n//npm.pkg.github.com/:_authToken=GITHUB_PACKAGES_READ_TOKEN\" >> .npmrc",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "prebuild": "yarn generate:index && yarn generate:og && yarn generate:rss && yarn generate:sitemap",
    "start": "next start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^0.10.10",
    "@maximeheckel/design-system": "^0.1.2",
    "@visx/clip-path": "1.6.1",
    "@visx/curve": "1.6.1",
    "@visx/drag": "2.1.2",
    "@visx/event": "1.6.1",
    "@visx/group": "1.6.1",
    "@visx/responsive": "1.6.1",
    "@visx/scale": "1.6.1",
    "@visx/shape": "1.6.1",
    "@visx/tooltip": "1.6.1",
    "chalk": "4.1.1",
    "d3-array": "2.12.0",
    "date-fns": "2.17.0",
    "detect-gpu": "^4.0.5",
    "focus-trap-react": "8.3.2",
    "framer-motion": "6.2.4",
    "globby": "11.0.2",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "gray-matter": "4.0.2",
    "lodash.merge": "4.6.2",
    "lunr": "2.3.9",
    "next": "12.0.9",
    "next-mdx-remote": "3.0.2",
    "next-seo": "4.29.0",
    "playwright": "1.17.1",
    "playwright-aws-lambda": "0.7.0",
    "playwright-core": "1.17.1",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.25.0",
    "qs": "6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "8.27.1",
    "react-live": "2.2.3",
    "react-query": "2.25.2",
    "reading-time": "1.2.1",
    "remark-autolink-headings": "6.0.1",
    "remark-slug": "6.0.0",
    "rss": "1.2.2",
    "unist-util-find-after": "3.0.0",
    "unist-util-visit": "2.0.3",
    "unist-util-visit-parents": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@netlify/plugin-nextjs": "4.2.2",
    "@next/bundle-analyzer": "10.0.5",
    "@now/node": "1.8.5",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3-array": "2.9.0",
    "@types/jest": "26.0.19",
    "@types/lodash.merge": "^4.6.6",
    "@types/lunr": "^2.3.3",
    "@types/node": "14.14.20",
    "@types/qs": "6.9.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "cross-env": "7.0.3",
    "cypress": "9.1.1",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.3",
    "jest-next-dynamic": "1.0.1",
    "netlify-plugin-cache-nextjs": "1.5.2",
    "next-remote-watch": "^1.0.0",
    "prettier": "2.2.1",
    "typescript": "4.3.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeHeckel/blog.maximeheckel.com"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": [
        "babel-jest",
        {
          "configFile": "./jest-babel-config.json"
        }
      ]
    },
    "transformIgnorePatterns": [],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "@theme/(.*)": "<rootDir>/core/$1",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}

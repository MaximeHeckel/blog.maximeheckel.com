{
  "name": "blog.maximeheckel.com",
  "private": true,
  "description": "blog.maximeheckel.com - Blog",
  "version": "0.5.0",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "@emotion/react": "11.1.4",
    "@emotion/server": "11.0.0",
    "@emotion/styled": "11.0.0",
    "critters": "0.0.6",
    "focus-trap-react": "8.3.2",
    "framer-motion": "3.1.1",
    "globby": "11.0.2",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "gray-matter": "4.0.2",
    "mousetrap": "1.6.5",
    "next": "10.0.4",
    "next-mdx-remote": "2.0.1",
    "next-seo": "4.17.0",
    "playwright": "1.8.0",
    "playwright-aws-lambda": "0.6.0",
    "playwright-core": "1.8.0",
    "plyr-react": "3.0.7",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "qs": "6.9.4",
    "react": "17.0.0",
    "react-anchor-link-smooth-scroll": "1.0.12",
    "react-dom": "17.0.0",
    "react-intersection-observer": "8.27.1",
    "react-live": "2.2.3",
    "react-query": "2.25.2",
    "react-scrollspy": "3.4.3",
    "reading-time": "1.2.1",
    "recharts": "2.0.7",
    "remark-autolink-headings": "6.0.1",
    "remark-slug": "6.0.0",
    "rss": "1.2.2",
    "unist-util-find-after": "3.0.0",
    "unist-util-visit": "2.0.3",
    "unist-util-visit-parents": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@cypress/snapshot": "2.1.7",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@emotion/jest": "11.1.0",
    "@next/bundle-analyzer": "10.0.5",
    "@now/node": "1.8.5",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@types/jest": "26.0.19",
    "@types/mousetrap": "1.6.5",
    "@types/node": "14.14.20",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-scrollspy": "3.3.3",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "babel-jest": "26.6.3",
    "cross-env": "7.0.3",
    "cypress": "6.2.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeHeckel/blog.maximeheckel.com"
  },
  "resolutions": {
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}

  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build:dev": " next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "dev": "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) next dev",
    "export": "next export",
    "check-format": "prettier --check \"./**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "generate:og": "node ./scripts/generate-opengraph-images",
    "generate:rss": "node ./scripts/generate-rss",
    "generate:sitemap": "node ./scripts/generate-sitemap",
    "link-ds": "rm -rf .next && pnpm link --global @maximeheckel/design-system",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "prebuild": "pnpm generate:rss && pnpm generate:sitemap",
    "start": "next start",
    "test": "is-ci \"test:run\" \"test:watch\"",
    "test:run": "TZ=utc vitest run",
    "test:watch": "TZ=utc vitest",
    "type-check": "tsc --noEmit",
    "unlink-ds": "rm -rf .next && pnpm unlink && pnpm install"
  },

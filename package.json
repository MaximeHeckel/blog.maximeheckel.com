{
  "name": "blog.maximeheckel.com",
  "private": true,
  "description": "blog.maximeheckel.com - Blog",
  "version": "0.5.0",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build:dev": "next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx}\"",
    "generate:og": "node ./scripts/generate-opengraph-images",
    "generate:rss": "node ./scripts/generate-rss",
    "generate:sitemap": "node ./scripts/generate-sitemap",
    "link-ds": "rm -rf .next && pnpm link --global @maximeheckel/design-system",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "prebuild": "pnpm generate:rss && pnpm generate:sitemap",
    "start": "next start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "unlink-ds": "rm -rf .next && pnpm unlink && pnpm install"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "1.18.4",
    "@maximeheckel/design-system": "^2.1.6",
    "@radix-ui/react-dialog": "^1.0.5",
    "@supabase/supabase-js": "^2.39.1",
    "@vercel/analytics": "^0.1.11",
    "@vercel/edge": "^0.3.4",
    "@vercel/kv": "^0.2.4",
    "@vercel/og": "^0.5.20",
    "@visx/clip-path": "1.6.1",
    "@visx/curve": "1.6.1",
    "@visx/drag": "2.1.2",
    "@visx/event": "1.6.1",
    "@visx/group": "1.6.1",
    "@visx/heatmap": "^1.17.1",
    "@visx/responsive": "1.6.1",
    "@visx/scale": "1.6.1",
    "@visx/shape": "1.6.1",
    "@visx/tooltip": "1.6.1",
    "chalk": "4.1.1",
    "d3-array": "2.12.0",
    "date-fns": "2.17.0",
    "detect-gpu": "^4.0.50",
    "eventsource-parser": "^1.1.1",
    "framer-motion": "10.12.3",
    "globby": "11.0.2",
    "gpt3-tokenizer": "^1.1.5",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "gray-matter": "4.0.2",
    "lodash.merge": "4.6.2",
    "next": "^14.0.4",
    "next-mdx-remote": "4.2.1",
    "next-seo": "5.5.0",
    "openai": "^3.3.0",
    "playwright-core": "1.17.1",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "qs": "6.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "8.27.1",
    "react-query": "2.25.2",
    "reading-time": "1.2.1",
    "remark-autolink-headings": "6.0.1",
    "remark-slug": "6.0.0",
    "rss": "1.2.2",
    "unist-util-find-after": "3.0.0",
    "unist-util-visit": "2.0.3",
    "unist-util-visit-parents": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@next/bundle-analyzer": "14.2.13",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3-array": "2.9.0",
    "@types/jest": "26.0.19",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "14.14.20",
    "@types/qs": "6.9.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "28.1.3",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "cross-env": "7.0.3",
    "cypress": "^13.7.1",
    "dotenv": "^16.3.1",
    "eslint": "7.29.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "playwright": "1.17.1",
    "playwright-webkit": "^1.40.1",
    "prettier": "2.2.1",
    "progress": "^2.0.3",
    "typescript": "^4.9.5"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeHeckel/blog.maximeheckel.com"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": [
        "babel-jest",
        {
          "configFile": "./jest-babel-config.json"
        }
      ]
    },
    "transformIgnorePatterns": [],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/core/$1",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
